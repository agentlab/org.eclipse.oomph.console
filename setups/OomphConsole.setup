<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xmlns:workingsets="http://www.eclipse.org/oomph/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore http://www.eclipse.org/oomph/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/WorkingSets.ecore"
    name="oomph.console"
    label="Oomph Installer Console">
  <annotation
      source="http://www.eclipse.org/oomph/setup/BrandingInfo">
    <detail
        key="imageURI">
      <value>http://www.eclipse.org/downloads/images/oomph-42x42.png</value>
    </detail>
    <detail
        key="siteURI">
      <value>https://projects.eclipse.org/projects/tools.oomph</value>
    </detail>
  </annotation>
  <stream name="master"
      label="master">
    <setupTask
        xsi:type="git:GitCloneTask"
        id="git.clone.oomph.console"
        remoteURI="agentlab/org.eclipse.oomph.console"
        checkoutBranch="master"
        userID="">
      <annotation
          source="http://www.eclipse.org/oomph/setup/InducedChoices">
        <detail
            key="inherit">
          <value>github.remoteURIs</value>
        </detail>
        <detail
            key="label">
          <value>${scope.project.label} Github repository</value>
        </detail>
        <detail
            key="target">
          <value>remoteURI</value>
        </detail>
      </annotation>
      <description>Oomph</description>
    </setupTask>
    <setupTask
        xsi:type="setup.targlets:TargletTask">
      <targlet
          name="Oomph"
          activeRepositoryList="${eclipse.target.platform}"
          includeAllPlatforms="true">
        <annotation
            source="http:/www.eclipse.org/oomph/targlets/PomArtifactUpdater">
          <detail
              key="skipArtifactIDs">
            <value>false</value>
          </detail>
          <detail
              key="skipVersions">
            <value>false</value>
          </detail>
        </annotation>
        <annotation
            source="http:/www.eclipse.org/oomph/targlets/TargetDefinitionGenerator">
          <detail
              key="location">
            <value>${git.clone.oomph.console.location/releng/org.eclipse.oomph.setup.installer.console.releng/org.eclipse.oomph.setup.installer.console.releng.target}</value>
          </detail>
          <detail
              key="extraUnits">
            <value>org.eclipse.equinox.executable.feature.group</value>
          </detail>
          <detail
              key="singleLocation">
            <value>true</value>
          </detail>
          <detail
              key="includeAllPlatforms">
            <value>false</value>
          </detail>
          <detail
              key="includeSource">
            <value>true</value>
          </detail>
        </annotation>
        <requirement
            name="org.eclipse.userstorage"/>
        <requirement
            name="org.eclipse.oomph.setup.sdk.feature.group"/>
        <requirement
            name="org.eclipse.equinox.core.sdk.feature.group"/>
        <requirement
            name="org.eclipse.equinox.sdk.feature.group"/>
        <requirement
            name="*"/>
        <sourceLocator
            rootFolder="${git.clone.oomph.console.location}"/>
        <repositoryList
            name="Oxygen">
          <repository
              url="http://download.eclipse.org/oomph/uss/updates/latest"/>
          <repository
              url="http://download.eclipse.org/egit/updates"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/emf/updates/2.13milestones"/>
          <repository
              url="http://download.eclipse.org/mylyn/snapshots/3.22"/>
          <repository
              url="http://download.eclipse.org/technology/m2e/snapshots/1.8.0/latest"/>
          <repository
              url="http://download.eclipse.org/technology/nebula/snapshot"/>
          <repository
              url="http://download.eclipse.org/technology/swtbot/snapshots"/>
          <repository
              url="http://download.eclipse.org/webtools/repository/oxygen"/>
          <repository
              url="http://download.eclipse.org/oomph/updates/milestone/latest"/>
        </repositoryList>
        <repositoryList
            name="Neon">
          <repository
              url="http://download.eclipse.org/releases/neon"/>
          <repository
              url="http://download.eclipse.org/eclipse/updates/4.6"/>
          <repository
              url="http://download.eclipse.org/technology/nebula/snapshot"/>
          <repository
              url="http://download.eclipse.org/technology/swtbot/releases/latest"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
          <repository
              url="http://download.eclipse.org/oomph/drops/release/1.6.0"/>
        </repositoryList>
        <repositoryList
            name="Mars">
          <repository
              url="http://download.eclipse.org/releases/mars"/>
          <repository
              url="http://download.eclipse.org/technology/nebula/snapshot"/>
          <repository
              url="http://download.eclipse.org/technology/swtbot/releases/latest"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
          <repository
              url="http://download.eclipse.org/oomph/uss/updates/latest"/>
        </repositoryList>
        <repositoryList
            name="Luna">
          <repository
              url="http://download.eclipse.org/releases/luna"/>
          <repository
              url="http://download.eclipse.org/technology/swtbot/releases/latest"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
          <repository
              url="http://download.eclipse.org/oomph/uss/updates/latest"/>
        </repositoryList>
        <repositoryList
            name="Kepler">
          <repository
              url="http://download.eclipse.org/releases/kepler"/>
          <repository
              url="http://download.eclipse.org/technology/swtbot/releases/latest"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
          <repository
              url="http://download.eclipse.org/oomph/uss/updates/latest"/>
        </repositoryList>
        <repositoryList
            name="Juno">
          <repository
              url="http://download.eclipse.org/releases/juno"/>
          <repository
              url="http://download.eclipse.org/technology/swtbot/releases/latest"/>
          <repository
              url="http://download.eclipse.org/modeling/emf/cdo/orbit/latest-R"/>
          <repository
              url="http://download.eclipse.org/oomph/uss/updates/latest"/>
        </repositoryList>
      </targlet>
    </setupTask>
    <setupTask
        xsi:type="setup.workingsets:WorkingSetTask">
      <workingSet
          name="Oomph Console">
        <predicate
            xsi:type="predicates:AndPredicate">
          <operand
              xsi:type="predicates:NamePredicate"
              pattern="org\.eclipse\.oomph.*"/>
          <operand
              xsi:type="workingsets:ExclusionPredicate"/>
        </predicate>
      </workingSet>
      <workingSet
          name="Oomph Setup Models">
        <predicate
            xsi:type="predicates:NamePredicate"
            pattern="setups.*"/>
      </workingSet>
    </setupTask>
  </stream>
  <logicalProjectContainer
      xsi:type="setup:Project"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='ru.agentlab']/@projects[name='osgi']"/>
  <description>The Oomph project provides tools and frameworks to enhance the installation and usage of Eclipse.</description>
</setup:Project>
